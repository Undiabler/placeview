		</level>
		<level tiledimagewidth="2048" tiledimageheight="2048">
			<cube url="pano_1405.tiles/l2/_%s_%v_%h.jpg" />
		</level>
		<level tiledimagewidth="4096" tiledimageheight="4096">
			<cube url="pano_1405.tiles/l1/_%s_%v_%h.jpg" />
		</level>
		<mobile>
			<cube url="pano_1405.tiles/mobile_%s.jpg" />
		</mobile>
	</image>

</krpano>
